x = int(input("masukan jumlah boneka: "))
if x <=12:
    print("harga perboneka Rp 20000 ")
    print("total", x * 20.000)
elif x <=24:
    print("harga perboneka Rp 19500")
    print("total", x * 19.500)
elif x <=50:
    print("harga perboneka Rp 18000")
    print("total", x * 18.000)
else:
    print("harga perboneka Rp 17000")
    print("total", x * 17000)